   "source": [
    "from nltk.stem import RegexpStemmer"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "77fb4cc2",
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_stems(text):#正则化\n",
    "    regex_stemmer = RegexpStemmer('ing$', min=4)\n",
    "    \n",
    "    return ' '.join([regex_stemmer.stem(wd) for wd in text.split()])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "73ec4df2",
